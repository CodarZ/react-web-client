{
  "name": "react-web-client",
  "version": "0.0.0",
  "type": "module",
  "author": "CodarZ",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "type:check": "tsc --noEmit",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:mock": "tsc -b && vite build --mode development",
    "preview": "vite preview",
    "format:check": "prettier --check . --log-level warn",
    "format:fix": "prettier --write . | sed '/(unchanged)/d'",
    "lint:check": "biome check .",
    "lint:fix": "biome check --write --unsafe --no-errors-on-unmatched",
    "msw:init": "msw init public",
    "api:gen": "orval"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-router": "^1.154.6",
    "ahooks": "^3.9.6",
    "antd": "^6.2.1",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "nprogress": "^0.2.0",
    "radash": "^12.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@faker-js/faker": "^10.2.0",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router-devtools": "^1.154.6",
    "@tanstack/router-plugin": "^1.154.6",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.7",
    "orval": "^8.0.2",
    "prettier": "^3.8.1",
    "prettier-plugin-css-order": "^2.2.0",
    "typescript": "~5.9.3",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^7.3.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
